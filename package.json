    "scripts": {
        "build:css": "postcss src/index.pcss -o src/index.css",
        "watch:css": "postcss src/index.pcss -o src/index.css -w",
        "lint:old": "yarn style:lint && eslint --ext .ts,.tsx src/",
        "lint": "eslint --ext .ts,.tsx src/redux/ src/app/ src/lib/ src/context",
        "lint-fix": "eslint --ext .ts,.tsx src/ --fix",
        "style:lint": "npx stylelint \"src/**/*.pcss\"",
        "mockAPI": "node mocks/mockAPI.js",
        "mockWS": "node mocks/mockWS.js",
        "start:old": "npm-run-all -p watch:css start-js",
        "start": "cross-env NODE_OPTIONS=--max_old_space_size=2048 webpack-dev-server --config ./webpack/development.ts",
        "build": "cross-env NODE_ENV=production && cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --optimize-minimize --config ./webpack/production.ts",
        "start-mock": "npm-run-all -p watch:css start-js mockAPI mockWS",
        "start-js": "react-scripts start",
        "build-js": "react-app-rewired build --scripts-version react-scripts build",
        "build:old": "npm-run-all build:css build-js clean:mapfiles",
        "clean:mapfiles": "find build -name '*.map' -exec rm {} ';'",
        "test": "jest --config jest.config.js",
        "test:coverage": "react-scripts test --collectCoverage=true",
        "test:ci": "jest --config jest.config.js",
        "test:ci:old": "react-scripts test --ci",
        "spec:integration": "cypress open",
        "eject": "react-scripts eject",
        "clean": "rimraf build",
        "analyze": "source-map-explorer build/static/js/main.*"
    },
