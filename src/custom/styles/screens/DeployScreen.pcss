.pg-deploy {
  align-items: flex-start !important;
  flex-direction: column;
  justify-content: flex-start !important;
  margin-top: calc(var(--gap) * -1.3) !important;
  min-height: 100vh;
  padding: 25px 0;

  &__row-wrap {
    align-items: flex-start;
    display: flex;
    flex-direction: row;
    height: max-content;
    justify-content: space-between;
    width: 100%;
  }
}
